<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="typefaces" format="enum">
        <enum name="BTitr" value="1" />
        <enum name="BYekan" value="2" />
        <enum name="BTraffic" value="3" />
        <enum name="BTrafficBold" value="4" />
        <enum name="BZarBold" value="5" />
    </attr>
    <attr name="cornerRadius" format="dimension" />
    <attr name="backgroundColor" format="color" />
    <declare-styleable name="Button">
        <attr name="android:text" format="string" />
        <attr name="cornerRadius" />
        <attr name="backgroundColor" />
        <attr name="typefaces" />
    </declare-styleable>
    <declare-styleable name="Toolbar">
        <attr name="title" format="string" />
        <attr name="subtitle" format="string" />
        <attr name="backgroundColor" />
    </declare-styleable>
    <declare-styleable name="CornerRadius">
        <attr name="cornerRadius" />
        <attr name="backgroundColor" />
    </declare-styleable>
</resources>